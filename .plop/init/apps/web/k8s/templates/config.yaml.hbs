apiVersion: v1
kind: ConfigMap
metadata:
  name: \{{ include "{{kebabCase name}}-web.fullname" . }}
  labels:
    \{{- include "{{kebabCase name}}-web.labels" . | nindent 4 }}
data:
  PORT: \{{ .Values.service.port | quote }}
  PORT_HEALTH: \{{ .Values.health.port | quote }}

  {{envPrefix}}_API_HOST: \{{ .Values.api.host }}
